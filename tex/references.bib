booktitle = {7th International Conference on Learning Representations, ICLR 2019},
eprint = {1811.01848},
file = {:home/mspahn/phd/research/lib/paper/Lowrey et al.{\_}2019{\_}Plan online, learn offline Efficient learning and exploration via model-based control.pdf:pdf},
month = {nov},
publisher = {International Conference on Learning Representations, ICLR},
title = {{Plan online, learn offline: Efficient learning and exploration via model-based control}},
url = {http://arxiv.org/abs/1811.01848},
year = {2019}
}
@article{Korayem2015,
abstract = {This work proposes an algorithm besides output feedback linearization method for controlling a wheeled mobile robot with two manipulators. To control such a mobile robot, three desired trajectories (or fixed points for regulation) are needed: two for the arms and one for the base. Improper choice in base path leads to singularity, low performance and failure. To prevent singularities and to attain a smooth motion, an algorithm is introduced as a local path planning process for the base. It consists of two parts which helps the robot to maintain the desired configuration. The first part of the algorithm mainly focuses on the position and orientation of the base in future time based on the arms configuration, and the second part adjusts the movement of the arms for obtaining a consistent motion. This maintenance reduces and preserves the norm of applied torques, which consequently leads to an increase in the performance of the robot and its dynamic load carrying capacity (DLCC). Also, it is no longer required to define a trajectory or an end point for the base movement since they will be calculated automatically by the algorithm.},
annote = {Investigating the case of a wheeled mobile manipulators with two mounted arms, the goal was to find a control algorithms that is used to help the execution of a point-to-point motion. The used improvement relies on a local path planner for the base that respects the position of the arms to avoid singular configurations.},
author = {Korayem, M. H. and Esfeden, R. Abbasi and Nekoo, S. R.},
doi = {10.1007/s12206-015-0349-x},
file = {:home/mspahn/phd/research/lib/paper/Korayem, Esfeden, Nekoo{\_}2015{\_}Path planning algorithm in wheeled mobile manipulators based on motion of arms.pdf:pdf},
issn = {1738494X},
journal = {Journal of Mechanical Science and Technology},
keywords = {DLCC,Locally path planning,Multiple arms,Non-holonomic constraints,Output feedback linearization,Wheeled mobile robot},
month = {apr},
