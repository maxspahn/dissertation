file = {:home/mspahn/phd/research/lib/paper/Kesper et al.{\_}2015{\_}A generic approach to self-localization and mapping of mobile robots without using a kinematic model.pdf:pdf},
isbn = {9783319224152},
issn = {16113349},
keywords = {Hexapod robot,Inertial measurement unit,Laser range finder,Mobile robots,Probabilistic robotics,SLAM},
pages = {136--142},
