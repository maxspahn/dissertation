@article{Kalakrishnan2011,
abstract = {We present a new approach to motion planning using a stochastic trajectory optimization framework. The approach relies on generating noisy trajectories to explore the space around an initial (possibly infeasible) trajectory, which are then combined to produced an updated trajectory with lower cost. A cost function based on a combination of obstacle and smoothness cost is optimized in each iteration. No gradient information is required for the particular optimization algorithm that we use and so general costs for which derivatives may not be available (e.g. costs corresponding to constraints and motor torques) can be included in the cost function. We demonstrate the approach both in simulation and on a mobile manipulation system for unconstrained and constrained tasks. We experimentally show that the stochastic nature of STOMP allows it to overcome local minima that gradient-based methods like CHOMP can get stuck in. {\textcopyright} 2011 IEEE.},
annote = {Developping a stochastic planner to respect several constraints without the need for them to be differentiable. 
Using white noise around the state and possibly discrete constraints.
The basic algorithm:
Repeat until convergence: 
1) Create noisy trajectories
2) Compute costs for each trajectory
3) Compute gradient based on the expectation formula
4) Update trajectory
5) Compute its costs

Updates are scaled to make sure no huge changes are made in the loop

An example of manipulation of a 7-DoF is given.
Joint limits are respected by clipping generated noisy trajectories at the beginning.
This algorithm is able to respect constraints which are needed to hold a glass of water for example.},
author = {Kalakrishnan, Mrinal and Chitta, Sachin and Theodorou, Evangelos and Pastor, Peter and Schaal, Stefan},
doi = {10.1109/ICRA.2011.5980280},
file = {:home/mspahn/phd/research/lib/paper/Kalakrishnan et al.{\_}2011{\_}STOMP Stochastic trajectory optimization for motion planning.pdf:pdf},
isbn = {9781612843865},
issn = {10504729},
journal = {Proceedings - IEEE International Conference on Robotics and Automation},
keywords = {arm,motionPlanning,obstacleAvoidance,ros,samplingBased,stochasticAlgorithm,stomp},
mendeley-tags = {arm,motionPlanning,obstacleAvoidance,ros,samplingBased,stochasticAlgorithm,stomp},
pages = {4569--4574},
title = {{STOMP: Stochastic trajectory optimization for motion planning}},
url = {https://wiki.ros.org/Papers/ICRA2011{\_}Kalakrishnan},
year = {2011}
}
@inproceedings{Trinh2019,
abstract = {This paper presents a novel path planning system for multiple robots working in an uncontrolled environment in the presence of humans. The approach combines the use of Petri net to plan the movement of multiple robots to prevent the risk of congestion caused by routing several robots into a narrow region, together with a dipole field with dynamic window approach to avoid collisions of a robot with dynamic obstacles. By regarding the velocity and direction of both humans and robots as a source of magnetic dipole moment, the dipole-dipole interaction between the moving objects will generate repulsive forces to prevent collisions. The whole system is presented on robot operating system platform so that its implementation can be extendable into real robots. Experimental results with Gazebo simulator demonstrates the effectiveness of the proposed approach.},
annote = {Petri-Net approach.},
author = {Trinh, Lan Anh and Ekstrom, Mikael and Curuklu, Baran},
booktitle = {2019 6th International Conference on Control, Decision and Information Technologies, CoDIT 2019},
doi = {10.1109/CoDIT.2019.8820359},
file = {:home/mspahn/phd/research/lib/paper/Trinh, Ekstrom, Curuklu{\_}2019{\_}Petri net based navigation planning with dipole field and dynamic window approach for collision avoidance.pdf:pdf},
isbn = {9781728105215},
month = {apr},
pages = {1013--1018},
publisher = {Institute of Electrical and Electronics Engineers Inc.},
title = {{Petri net based navigation planning with dipole field and dynamic window approach for collision avoidance}},
year = {2019}
}
@inproceedings{Hauser2008,
abstract = {In this paper we study the quasi-static motion of large legged robots that have many degrees of freedom. While gaited walking may suffice on easy ground, rough and steep terrain requires unique sequences of footsteps and postural adjustments specifically adapted to the terrain's local geometric and physical properties. In this paper we present a planner that computes these motions by combining graph searching to generate a sequence of candidate footfalls with probabilistic sample-based planning to generate continuous motions that reach these footfalls. To improve motion quality, the probabilistic planner derives its sampling strategy from a small set of motion primitives that have been generated offline. The viability of this approach is demonstrated in simulation for the six-legged Lunar vehicle ATHLETE and the humanoid HRP-2 on several example terrains, including one that requires both hand and foot contacts and another that requires rappelling. {\textcopyright} 2008 SAGE Publications.},
author = {Hauser, Kris and Bretl, Timothy and Latombe, Jean Claude and Harada, Kensuke and Wilcox, Brian},
booktitle = {International Journal of Robotics Research},
doi = {10.1177/0278364908098447},
file = {:home/mspahn/phd/research/lib/paper/Hauser et al.{\_}2008{\_}Motion planning for legged robots on varied terrain.pdf:pdf},
issn = {02783649},
keywords = {Humanoids,Legged robots,Motion planning,Motion primitives,Probabilistic sample-based planning},
month = {nov},
number = {11-12},
pages = {1325--1349},
title = {{Motion planning for legged robots on varied terrain}},
volume = {27},
year = {2008}
}
@article{Zucker2013,
abstract = {In this paper, we present CHOMP (covariant Hamiltonian optimization for motion planning), a method for trajectory optimization invariant to reparametrization. CHOMP uses functional gradient techniques to iteratively improve the quality of an initial trajectory, optimizing a functional that trades off between a smoothness and an obstacle avoidance component. CHOMP can be used to locally optimize feasible trajectories, as well as to solve motion planning queries, converging to low-cost trajectories even when initialized with infeasible ones. It uses Hamiltonian Monte Carlo to alleviate the problem of convergence to high-cost local minima (and for probabilistic completeness), and is capable of respecting hard constraints along the trajectory. We present extensive experiments with CHOMP on manipulation and locomotion tasks, using seven-degree-of-freedom manipulators and a rough-terrain quadruped robot. {\textcopyright} The Author(s) 2013.},
annote = {Based on the idea that sampling-based planning can outperform randomized approaches, sampling is performed directly in the space of trajectories. CHOMP, the proposed algorithm, uses a smoothness functional and and an obstacle functional. Derivative are found for both functionals and a gradient descent technique can be applied. Problems occur when dealing with narrow passages and it might actual be difficult to find the right cost functions. It has not been tested in real environments.},
author = {Zucker, Matt and Ratliff, Nathan and Dragan, Anca D. and Pivtoraiko, Mihail and Klingensmith, Matthew and Dellin, Christopher M. and Bagnell, J. Andrew and Srinivasa, Siddhartha S.},
doi = {10.1177/0278364913488805},
file = {:home/mspahn/phd/research/lib/paper/Zucker et al.{\_}2013{\_}CHOMP Covariant Hamiltonian optimization for motion planning.pdf:pdf},
issn = {02783649},
journal = {International Journal of Robotics Research},
keywords = {Motion planning,constrained optimization,distance fields},
month = {aug},
number = {9-10},
