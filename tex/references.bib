isbn = {9781612843865},
issn = {10504729},
pages = {6115--6120},
title = {{Cart pushing with a mobile manipulation system: Towards navigation with moveable objects}},
year = {2011}
}
@article{Suh1988,
abstract = {An approach to robot-path planning is developed by considering both the traveling distance and the safety of the robot. Incorporation of robot safety into path planning is important not only because of the uncertainties in the robot dynamics during path execution, but also because of the inaccuracies in the geometric modeling of obstacles. A computationally efficient algorithm is developed to find a near-optimal path with a weighted distance-safety criterion by using a variational calculus and dynamic programming (VCDP) method. The algorithm is readily applicable to any factory environment by representing the free workspace as channels. A method for deriving these channels is also proposed. Although it is developed mainly for two-dimensional (2D) problems, our method can be easily extended to a class of three-dimensional (3D) problems. Numerical examples are presented to demonstrate the utility and power of this method. {\textcopyright} 1988 IEEE},
author = {Suh, Suk Hwan and Shin, Kang G.},
doi = {10.1109/56.794},
file = {:home/mspahn/phd/research/lib/paper/Suh, Shin{\_}1988{\_}A Variational Dynamic Programming Approach to Robot-Path Planning With a Distance-Safety Criterion.pdf:pdf},
issn = {08824967},
journal = {IEEE Journal on Robotics and Automation},
number = {3},
pages = {334--349},
title = {{A Variational Dynamic Programming Approach to Robot-Path Planning With a Distance-Safety Criterion}},
volume = {4},
year = {1988}
