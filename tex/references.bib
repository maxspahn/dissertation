file = {:home/mspahn/phd/research/lib/paper/Wei, Ren{\_}2018{\_}A method on dynamic path planning for robotic manipulator autonomous obstacle avoidance based on an improved RRT algorithm.pdf:pdf},
issn = {14248220},
journal = {Sensors (Switzerland)},
keywords = {Autonomous obstacle avoidance,Dynamic path planning,Dynamic unstructured environment,RRT algorithm,Robotic manipulator},
month = {feb},
