@article{Alonso-Mora2017,
abstract = {We present a constrained optimization method for multi-robot formation control in dynamic environments, where the robots adjust the parameters of the formation, such as size and three-dimensional orientation, to avoid collisions with static and moving obstacles, and to make progress towards their goal. We describe two variants of the algorithm, one for local motion planning and one for global path planning. The local planner first computes a large obstacle-free convex region in a neighborhood of the robots, embedded in position-time space. Then, the parameters of the formation are optimized therein by solving a constrained optimization, via sequential convex programming. The robots navigate towards the optimized formation with individual controllers that account for their dynamics. The idea is extended to global path planning by sampling convex regions in free position space and connecting them if a transition in formation is possible - computed via the constrained optimization. The path of lowest cost to the goal is then found via graph search. The method applies to ground and aerial vehicles navigating in two- and three-dimensional environments among static and dynamic obstacles, allows for reconfiguration, and is efficient and scalable with the number of robots. In particular, we consider two applications, a team of aerial vehicles navigating in formation, and a small team of mobile manipulators that collaboratively carry an object. The approach is verified in experiments with a team of three mobile manipulators and in simulations with a team of up to sixteen Micro Air Vehicles (quadrotors).},
annote = {The constrained optimization problem is solved via sequential convex programming to navigate multi-robot formations in dynamic environments. The method can be used in local online planning problems and global path planning and the method scales up to a arbitrary number of agents.},
author = {Alonso-Mora, Javier and Baker, Stuart and Rus, Daniela},
doi = {10.1177/0278364917719333},
file = {:home/mspahn/phd/research/lib/paper/Alonso-Mora, Baker, Rus{\_}2017{\_}Multi-robot formation control and object transport in dynamic environments via constrained optimization.pdf:pdf},
issn = {17413176},
journal = {International Journal of Robotics Research},
keywords = {Multi-robot systems,collaborative mobile manipulators,collaborative object transport,constrained optimization,formation control,micro air vehicles,motion planning,sequential convex programming,team of aerial vehicles},
month = {aug},
number = {9},
pages = {1000--1021},
title = {{Multi-robot formation control and object transport in dynamic environments via constrained optimization}},
url = {http://journals.sagepub.com/doi/10.1177/0278364917719333},
volume = {36},
year = {2017}
}
@inproceedings{Pall2018,
abstract = {A robot with contact sensing capability can reduce uncertainty relative to the environment by deliberately moving into contact and matching the resulting contact measurement to different possible states in the world. We present a manipulation planner that finds and sequences these actions by reasoning explicitly about the uncertainty over the robot's state. The planner incrementally constructs a policy that covers all possible contact states during a manipulation and finds contingencies for each of them. In contrast to conformant planners (without contingencies), the planned contingent policies are more robust. We demonstrate this in simulated and real-world manipulation experiments. In contrast to POMDP-based planners, we show that our planner can be directly applied to high-dimensional configuration spaces.},
author = {P{\'{a}}ll, Elod and Sieverling, Arne and Brock, Oliver},
booktitle = {IEEE International Conference on Intelligent Robots and Systems},
doi = {10.1109/IROS.2018.8594365},
file = {:home/mspahn/phd/research/lib/paper/P{\'{a}}ll, Sieverling, Brock{\_}2018{\_}Contingent Contact-Based Motion Planning.pdf:pdf},
isbn = {9781538680940},
issn = {21530866},
month = {dec},
pages = {6615--6621},
publisher = {Institute of Electrical and Electronics Engineers Inc.},
title = {{Contingent Contact-Based Motion Planning}},
year = {2018}
}
@inproceedings{Tordesillas2019,
abstract = {High-speed trajectory planning through unknown environments requires algorithmic techniques that enable fast reaction times while maintaining safety as new information about the operating environment is obtained. The requirement of computational tractability typically leads to optimization problems that do not include the obstacle constraints (collision checks are done on the solutions) or use a convex decomposition of the free space and then impose an ad-hoc time allocation scheme for each interval of the trajectory. Moreover, safety guarantees are usually obtained by having a local planner that plans a trajectory with a final 'stop' condition in the freeknown space. However, these two decisions typically lead to slow and conservative trajectories. We propose FASTER (Fast and Safe Trajectory Planner) to overcome these issues. FASTER obtains high-speed trajectories by enabling the local planner to optimize in both the free-known and unknown spaces. Safety guarantees are ensured by always having a feasible, safe back-up trajectory in the free-known space at the start of each replanning step. Furthermore, we present a Mixed Integer Quadratic Program formulation in which the solver can choose the trajectory interval allocation, and where a time allocation heuristic is computed efficiently using the result of the previous replanning iteration. This proposed algorithm is tested extensively both in simulation and in real hardware, showing agile flights in unknown cluttered environments with velocities up to 3.6 $\backslash$mathrm{\{}m{\}}/$\backslash$mathrm{\{}s{\}}.},
archivePrefix = {arXiv},
arxivId = {2001.04420},
author = {Tordesillas, Jesus and Lopez, Brett T and How, Jonathan P},
booktitle = {IEEE International Conference on Intelligent Robots and Systems},
doi = {10.1109/IROS40897.2019.8968021},
eprint = {2001.04420},
file = {:home/mspahn/phd/research/lib/paper/Tordesillas, Lopez, How{\_}2019{\_}FASTER Fast and Safe Trajectory Planner for Flights in Unknown Environments.pdf:pdf},
isbn = {9781728140049},
issn = {21530866},
pages = {1934--1940},
title = {{FASTER: Fast and Safe Trajectory Planner for Flights in Unknown Environments}},
url = {https://github.com/jtorde},
year = {2019}
}
@inproceedings{Zeng2017,
abstract = {Robot warehouse automation has attracted significant interest in recent years, perhaps most visibly in the Amazon Picking Challenge (APC) [1]. A fully autonomous warehouse pick-and-place system requires robust vision that reliably recognizes and locates objects amid cluttered environments, self-occlusions, sensor noise, and a large variety of objects. In this paper we present an approach that leverages multiview RGB-D data and self-supervised, data-driven learning to overcome those difficulties. The approach was part of the MIT-Princeton Team system that took 3rd- and 4th-place in the stowing and picking tasks, respectively at APC 2016. In the proposed approach, we segment and label multiple views of a scene with a fully convolutional neural network, and then fit pre-scanned 3D object models to the resulting segmentation to get the 6D object pose. Training a deep neural network for segmentation typically requires a large amount of training data. We propose a self-supervised method to generate a large labeled dataset without tedious manual segmentation. We demonstrate that our system can reliably estimate the 6D pose of objects under a variety of scenarios. All code, data, and benchmarks are available at http://apc.cs.princeton.edu/},
archivePrefix = {arXiv},
arxivId = {1609.09475},
author = {Zeng, Andy and Yu, Kuan Ting and Song, Shuran and Suo, Daniel and Walker, Ed and Rodriguez, Alberto and Xiao, Jianxiong},
booktitle = {Proceedings - IEEE International Conference on Robotics and Automation},
doi = {10.1109/ICRA.2017.7989165},
eprint = {1609.09475},
file = {:home/mspahn/phd/research/lib/paper/Zeng et al.{\_}2017{\_}Multi-view self-supervised deep learning for 6D pose estimation in the Amazon Picking Challenge.pdf:pdf},
isbn = {9781509046331},
issn = {10504729},
month = {jul},
pages = {1386--1393},
publisher = {Institute of Electrical and Electronics Engineers Inc.},
title = {{Multi-view self-supervised deep learning for 6D pose estimation in the Amazon Picking Challenge}},
year = {2017}
}
@article{Luna2019,
abstract = {Sampling-based algorithms are known for their ability to effectively compute paths for high-dimensional robots in relatively short times. The same algorithms, however, are also notorious for poor-quality solution paths, particularly as the dimensionality of the system grows. This work proposes a new probabilistically complete sampling-based algorithm, XXL, specially designed to plan the motions of high-dimensional mobile manipulators and related platforms. Using a novel sampling and connection strategy that guides a set of points mapped on the robot through the workspace, XXL scales to realistic manipulator platforms with dozens of joints by focusing the search of the robot's configuration space to specific degrees of freedom that affect motion in particular portions of the workspace. Simulated planning scenarios with the Robonaut2 platform and planar kinematic chains confirm that XXL exhibits competitive solution times relative to many existing works while obtaining execution-quality solution paths. Solutions from XXL are of comparable quality to cost-aware methods even though XXL does not explicitly optimize over any particular criteria, and are computed in an order of magnitude less time. Furthermore, observations about the performance of sampling-based algorithms on high-dimensional manipulator planning problems are presented that reveal a cautionary tale regarding two popular guiding heuristics used in these algorithms, indicating that a nearly random search may outperform the state-of-the-art when defining such heuristics is known to be difficult.},
annote = {The goal is to bring a mobile manipulator in a pre-grasp position for human like robots. The presented XXL algorithm is a sampling-based algorithm which can deal with high-DoF manipulators effectively. It is especially well suited for tree-like robots, such as humanoids.},
author = {Luna, Ryan and Moll, Mark and Badger, Julia and Kavraki, Lydia E},
doi = {10.1177/0278364919890408},
file = {:home/mspahn/phd/research/lib/paper/Luna et al.{\_}2019{\_}A scalable motion planner for high-dimensional kinematic systems.pdf:pdf},
issn = {17413176},
journal = {International Journal of Robotics Research},
keywords = {High-dimensional systems,Motion planning,Sampling-based planning,Workspace-guided sampling},
month = {dec},
pages = {027836491989040},
title = {{A scalable motion planner for high-dimensional kinematic systems}},
url = {http://journals.sagepub.com/doi/10.1177/0278364919890408},
year = {2019}
}
@inproceedings{Peng2019,
abstract = {For practical applications of mobile manipulation, the robot should have the ability to model the environment geometrically and analyze the object of interest. If there are people or other robots working around, then the robot also needs to have good adaptability to the dynamic environment. In this paper, we propose a SLAM system which can perform object-oriented semantic mapping of a dynamic environment adaptively. The SLAM system consists of six threads, which can eliminate the effects of dynamic objects, analyze models of previously unseen object instances and create a global three-dimensional map of the environment simultaneously. The proposed SLAM system is verified on the TUM RGB-D dataset. Furthermore, the system is applied in a mobile manipulator for object grasping applications and achieves satisfactory results.},
annote = {Using unsupervised object-oriented semantic SLAM system with six parallel threads, the approach is able to outperform in practical mobile manipulation tasks, because it focuses on the small objects it is interessted.},
author = {Peng, Jichao and Shi, Xiaoyu and Wu, Jianhua and Xiong, Zhenhua},
booktitle = {IEEE/ASME International Conference on Advanced Intelligent Mechatronics, AIM},
doi = {10.1109/AIM.2019.8868371},
file = {:home/mspahn/phd/research/lib/paper/Peng et al.{\_}2019{\_}An object-oriented semantic SLAM system towards dynamic environments for mobile manipulation.pdf:pdf},
