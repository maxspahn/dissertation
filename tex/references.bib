author = {Quillen, Deirdre and Jang, Eric and Nachum, Ofir and Finn, Chelsea and Ibarz, Julian and Levine, Sergey},
booktitle = {Proceedings - IEEE International Conference on Robotics and Automation},
doi = {10.1109/ICRA.2018.8461039},
eprint = {1802.10264},
file = {:home/mspahn/phd/research/lib/paper/Quillen et al.{\_}2018{\_}Deep reinforcement learning for vision-based robotic grasping A simulated comparative evaluation of off-policy metho.pdf:pdf},
isbn = {9781538630815},
issn = {10504729},
month = {sep},
pages = {6284--6291},
publisher = {Institute of Electrical and Electronics Engineers Inc.},
title = {{Deep reinforcement learning for vision-based robotic grasping: A simulated comparative evaluation of off-policy methods}},
year = {2018}
}
@inproceedings{Canny1988,
abstract = {We give a PSPACE algorithm for determining the signs of multivariate polynomials at the common zeros of a system of polynomial equations. One of the consequences of this result is that the "Generalized Movers1 Problem" in robotics drops from EXPTIME into PSPACE, and is therefore PSPACE-complete by a previous hardness result [Rei]. We also show that the existential theory of the real numbers can be decided in PSPACE. Other geometric problems that also drop into PSPACE include the 3-d Euclidean Shortest Path Problem, and the "2-d Asteroid Avoidance Problem" described in [RSL Our method combines the theorem of the primitive element from classical algebra with a symbolic polynomial evaluation lemma from [BKR]. A decision problem involving several algebraic numbers is reduced to a problem involving a single algebraic number or primitive element, which rationally generates all the given algebraic numbers. {\textcopyright} 1988 ACM.},
address = {New York, New York, USA},
author = {Canny, John},
booktitle = {Proceedings of the Annual ACM Symposium on Theory of Computing},
